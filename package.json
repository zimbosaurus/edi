{
  "name": "@zimbosaurus/edi",
  "version": "1.0.0",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.11.8"
  },
  "scripts": {
    "lib:build": "parcel build src/index.ts --out-dir build/lib --target node --public-url ./",
    "lib:watch": "parcel watch src/index.ts --out-dir build/lib --target node --public-url ./",
    "lib:run": "node -r source-map-support/register --inspect build/lib/index.js",
    "lib:dist": "tsc -p tsconfig.json",
    "lib": "yarn lib:build && yarn lib:run"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.3"
  }
}
